
<div class="form-group">
		<%= f.label :head_count %>
		<%= f.number_field :head_count ,placeholder:"Head Count", min: 1, class:'form-control' %>
	</div>

	<div class="form-group">
		<%= f.label :start_date %>
		<%= f.text_field :start_date, placeholder:"Start Date", id:'from', class:'form-control' %>
	</div>

	<div class="form-group">
		<%= f.label :end_date %>
		<%= f.text_field :end_date, placeholder:"End date", id:'to', class:'form-control' %>
	</div>

	<div class="form-group">
		<%= f.submit "Submit", class:"btn btn-success btn-lg" %>
</div>

