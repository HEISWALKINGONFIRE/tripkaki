
  <div id="filterrific_results">

    <div>
      <%= page_entries_info packages %>
    </div>

    <table class="table table-hover">
      <thead>
        <tr>
          <th>Title</th>
          <th>Country</th>
          <th>State</th>
        </tr>
      </thead>
      <% packages.each do |package| %>
          <tbody>
            <tr>
              <td><%= link_to package.title, package %></td>
              <td><%= package.country_name %></td>
              <td><%= package.state_name %></td>
            </tr>
          </tbody>
      <% end %>
    </table>
    <%= will_paginate packages %>
  </div>
