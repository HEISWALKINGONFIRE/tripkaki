<div id="filterrific_results">

  <div>
    <%= page_entries_info packages %>
  </div>

  <table>
    <tr>
      <th>Title</th>
      <th>Country</th>
      <th>State</th>
    </tr>
    <% packages.each do |package| %>
      <tr>
        <td><%= package.title %></td>
        <td><%= package.country %></td>
        <td><%= package.state %></td>
      </tr>
    <% end %>
  </table>
</div>

<%= will_paginate packages %>